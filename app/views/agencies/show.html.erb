<div class="page-header">
  <%= link_to agencies_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_agency_path(@agency), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show agency</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @agency.title %></dd>

  <dt>Reg number:</dt>
  <dd><%= @agency.reg_number %></dd>

  <dt>Address:</dt>
  <dd><%= @agency.address %></dd>

  <dt>Phone:</dt>
  <dd><%= @agency.phone %></dd>

</dl>
